@model CommonLibrary.ViewModels.ProfilePageViewModel

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor((model => model.User.UserId))
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.User.UserId)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.User.UserName)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.User.UserName)
    </dd>
    <dt class="col-sm-2">
        User type:
    </dt>
    <dd class="col-sm-10">
        @Model.User.GetType().ToString().Split(".")[1]
    </dd>
</dl>
